	</LinearLayout>
	<LinearLayout
	    xmlns:android="http://schemas.android.com/apk/res/android"
	    android:layout_width="fill_parent"
		android:orientation="horizontal"
	    android:layout_height="wrap_content" >
	    	<TextView android:layout_height="wrap_content" android:id="@+id/senteTextView"
				android:textSize="24px" android:layout_marginTop="6px" android:layout_width="wrap_content"></TextView>
			<TextView android:layout_height="wrap_content" android:text="-" android:layout_marginLeft="10dip" 
